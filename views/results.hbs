<div class="research_page">
    <div class="search_bar">
        <img class="logo research search_bar_element" src="img/google.png">
        <div class="search_bar_element">
            <form class="research">
                <input class="research search_bar_element" type="text">
                <button class="research search_bar_element">Search</button>
            </form>
        </div>
    </div>
    <div class="results">
        <div class="result_number">{{results.length}} results</div>

        {{#each results as |result|}}
            <div class="result">
                <div class="link_name">{{result.url}}</div>
                <div>
                    <h1>
                        <a href="{{result.url}}">{{result.title}}</a>
                    </h1>
                    <p>{{result.description}}</p>
                    <ul>
                        {{#each result.links as |link|}}
                            <li><a class="small_links" href="{{link.url}}" target="_blank">{{link.title}}</a></li>
                        {{/each}}
                    </ul>
                </div>
            </div>
        {{/each}}

    </div>
</div>